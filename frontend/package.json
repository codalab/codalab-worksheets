    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^11.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "nock": "^13.0.4",
    "prettier": "^1.19.1",
    "react-scripts": "^3.4.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "PORT=2700 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fifteen --coverage",
    "eject": "react-scripts eject",
    "check-ci": "CI=true eslint src && prettier --config ./.prettierrc --check 'src/**/*.js'",
    "format": "CI=true eslint src --fix && prettier --config ./.prettierrc --check 'src/**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --max-warnings=0",
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 10
      }
    }
  }
}
